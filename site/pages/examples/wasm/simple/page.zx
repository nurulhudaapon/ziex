var count: i32 = 0;
pub fn Page(ctx: zx.PageContext) zx.Component {
    count += 1;

    return (
        <div @allocator={ctx.arena}>
            <Counter @rendering={.client} initial={count} />
            <Counter @rendering={.client} initial={count + 10} />
            <Counter @rendering={.client} initial={count + 20} />
        </div>
    );
}

pub const Counter = @import("../../client_counter.zx").Counter;

const zx = @import("zx");
