pub fn Page(allocator: zx.Allocator) zx.Component {
    const greeting = zx.Component{ .text = "Hello!" };

    return (
        <section @allocator={allocator}>
            <p>Greeting: {greeting}</p>
            <div>
                {greeting}
            </div>
        </section>
    );
}

const zx = @import("zx");
