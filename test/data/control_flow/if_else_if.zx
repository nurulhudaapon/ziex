pub fn Page(allocator: zx.Allocator) zx.Component {
    const is_logged_in = false;
    const is_premium = false;
    return (
        <main @allocator={allocator}>
            {if (is_logged_in) (<p>Welcome, User!</p>) else if (is_premium) (<p>Welcome, Premium User!</p>) else (<p>Please log in to continue.</p>)}
        </main>
    );
}

const zx = @import("zx");
